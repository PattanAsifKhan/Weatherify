{
    "manifest_version": 2,
    "name": "Weather",
    "version": "1.1",
    "description": "Shows current Weather",
    "permissions": [
        "<all_urls>"
    ],
    "browser_action": {
        "browser_style": true,
        "default_title": "Weather",
        "default_popup": "popup.html"
    }
}